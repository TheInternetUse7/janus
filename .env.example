# Environment Variables
# Local dev: copy to .env.local
# Docker compose: copy to .env

# Required
DISCORD_TOKEN=your_discord_bot_token
FLUXER_TOKEN=your_fluxer_bot_token
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/janus

# Optional
REDIS_URL=redis://localhost:6379
POSTGRES_DB=janus
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
FLUXER_API_URL=https://api.fluxer.app
FLUXER_WS_URL=wss://gateway.fluxer.app
FLUXER_WEB_BASE_URL=https://fluxer.app
FLUXER_EDIT_UPDATE_TTL_SECONDS=604800
LOG_LEVEL=info
RATE_LIMIT_PER_CHANNEL=5
LOOP_HASH_TTL=10
CB_FAILURE_THRESHOLD=10
CB_RESET_TIMEOUT=60000
